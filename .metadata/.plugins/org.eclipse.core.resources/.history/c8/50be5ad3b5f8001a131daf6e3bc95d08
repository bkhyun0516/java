package type;

public class TypeEx2 {
	public void test() {
		
	}
	
	public static void main(String[] args) {
	/*
	 * 
	 * 자바 기본자료형
	 * - 정수형: 
	 *	크기 
	 * 		- byte : -128~127(1 byte = 8 bit)
	 * 		- short: 
	 * 		- int
	 * 		- long
	 * 	
	 */
		
	// 기본 자료형 byte형 변수 b를 선언
		byte b;
		
		b= 10;	
		System.out.println(b);
	

		
	} //main() end
	
}// TypeEx2 end
